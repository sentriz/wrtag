env WRTAG_PATH_FORMAT='albums/{{ .Release.Title }}/{{ .Track.Title }}{{ .Ext }}'

exec tag write 'albums/Kat Moda/01.flac'
exec tag write 'albums/Kat Moda/02.flac'
exec tag write 'albums/Kat Moda/03.flac'

